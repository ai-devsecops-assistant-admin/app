{
  "version": "1.0",
  "basePath": "/v1",
  "endpoints": [
    {
      "id": "users.list",
      "method": "GET",
      "path": "/users",
      "flow": "users.list.flow.yaml"
    },
    {
      "id": "users.get",
      "method": "GET",
      "path": "/users/:id",
      "flow": "users.get.flow.yaml"
    },
    {
      "id": "users.create",
      "method": "POST",
      "path": "/users",
      "flow": "users.create.flow.yaml"
    }
  ]
}
